module digital_clock(
  input rst,
  input clk,
  output reg [4:0] hours,
  output reg [5:0]minutes,
  output reg [5:0]seconds
);
  always@(posedge rst or posedge clk)begin
    if(rst)begin
      seconds<=0;
      minutes<=0;
      hours<=0;
    end else begin
      if (seconds==59)begin//increment seconds
        seconds <=0;
      if (minutes==59)begin //increment minutes
      minutes<=0;
        if  (hours==23)begin
      hours<=0; //increment hours
          end else begin
      hours<= hours+1;
          end
    end else begin
      minutes<=minutes+1;
    end
    end else begin
      seconds<= seconds +1;
    end
  end 
    end
    endmodule
// Code your testbench here
// or browse Examples
module digital_clock_tb;
  reg rst;
  reg clk;
  wire [4:0] hours;
  wire [5:0] minutes;
  wire [5:0] seconds;

  // Correct instantiation of DUT
  digital_clock uut (
    .rst(rst),
    .clk(clk),
    .hours(hours),
    .minutes(minutes),
    .seconds(seconds)
  );

  initial clk = 0;
  always #1 clk = ~clk;

  initial begin
    // âœ… Make sure the format specifiers match the signals!
    $monitor("%0t | rst=%b | clk=%b | hours=%0d | minutes=%0d | seconds=%0d",
              $time,     rst,     clk,       hours,         minutes,        seconds);
    rst = 1; #2;
    rst = 0; #5000;
    $finish;
  end
endmodule
